<style>
  #productos { display: grid; grid-template-columns: 1fr 1fr 1fr; gap: 20px; }
  #productos > div { background-color: #f2f2f2; padding: 20px; border-radius:
  5px; text-align: center; }
</style>

<div id="productos">
  {{#each products}}
    <div>
      <h3>{{title}}</h3>
      <h2>{{id}}</h2>
      <p>${{price}}</p>
      <p>stock:{{stock}}</p>
    </div>
  {{/each}}
</div>